.management_address_page {
  padding: 10rpx 30rpx;
}
.management_address_page .border_bottom {
  border-bottom: 1rpx solid #f5f5f5;
}
.management_address_page .management_item {
  padding: 20rpx 0;
  height: 125rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.management_address_page .management_item .item_left {
  min-width: 90%;
}
.management_address_page .management_item .item_left .left_top {
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.management_address_page .management_item .item_left .left_top .top_icon {
  background-color: #fff4e0;
  font-size: 20rpx;
  color: #ffa600;
  border-radius: 8rpx;
  padding: 3rpx 5rpx;
  margin-right: 5rpx;
}
.management_address_page .management_item .item_left .left_top .top_text {
  font-size: 28rpx;
  color: #333333;
}
.management_address_page .management_item .item_left .left_bottom {
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 8rpx;
  font-size: 26rpx;
  color: #333333;
}
.management_address_page .management_item .item_left .left_bottom .bottom_name {
  margin-right: 30rpx;
}
.management_address_page .management_item .item_right {
  font-size: 26rpx;
  color: #a1a1a1;
}
.management_address_page .management_button {
  margin: 65rpx auto;
  width: 690rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  border-radius: 15rpx;
  border: 2rpx solid #ffa600;
  color: #ffa600;
  font-size: 30rpx;
}

