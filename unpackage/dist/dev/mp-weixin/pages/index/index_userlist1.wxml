<view class="content"><block wx:for="{{list1}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['xiajidaili',['$0'],[[['list1','',index]]]]]]]}}" hidden="{{!(list1)}}" class="card_block" style="{{'background:'+(item.bg)+';'}}" bindtap="__e"><view class="card_block_two flex-between"><view class="tit_card_name flex_columns"><text class="tit_name_b">{{item.storeUsername}}</text><block wx:if="{{item.storeRole==0}}"><text>直系代理</text></block><block wx:if="{{item.storeRole==1}}"><text>总代理</text></block><block wx:if="{{item.storeRole==2}}"><text>省代理</text></block><block wx:if="{{item.storeRole==3}}"><text>市代理</text></block></view><view class="tit_card_details flex_columns"><view class="details_right"><image src="../../static/手机icon.png" mode="aspectFit"></image><text>{{item.storePhone}}</text></view><view class="details_right"><image src="../../static/网络icon.png" mode="aspectFit"></image><text>{{item.storeIdcard}}</text></view></view><block wx:if="{{item.isok==true}}"><view class="tit_card_xiaji flex-center"><image src="../../static/icon/right1.png" mode="aspectFit"></image></view></block></view></view></block><view hidden="{{!(list1.length<0)}}" class="null flex-center"><text>暂无代理信息</text></view></view>